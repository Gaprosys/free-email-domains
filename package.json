{
  "name": "free-email-domains",
  "description": "A comprehensive list of all free email domain providers",
  "homepage": "https://nicedoc.io/Kikobeats/free-email-domains",
  "version": "1.0.3",
  "main": "domains.json",
  "author": {
    "email": "josefrancisco.verdu@gmail.com",
    "name": "Kiko Beats",
    "url": "https://kikobeats.com"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kikobeats/free-email-domains.git"
  },
  "bugs": {
    "url": "https://github.com/Kikobeats/free-email-domains/issues"
  },
  "keywords": [
    "block",
    "domain",
    "domains",
    "email",
    "form",
    "free",
    "hubspot",
    "mail"
  ],
  "dependencies": {
    "cheerio": "~1.0.0-rc.3",
    "got": "~11.4.0",
    "json-future": "~2.2.4"
  },
  "devDependencies": {
    "coveralls": "latest",
    "git-dirty": "latest",
    "husky": "latest",
    "lint-staged": "latest",
    "lodash": "latest",
    "mocha": "latest",
    "nyc": "latest",
    "prettier-standard": "latest",
    "should": "latest",
    "standard": "latest",
    "standard-markdown": "latest",
    "@commitlint/cli": "latest",
    "@commitlint/config-conventional": "latest",
    "ci-publish": "latest",
    "conventional-github-releaser": "latest",
    "finepack": "latest",
    "git-authors-cli": "latest",
    "npm-check-updates": "latest",
    "standard-version": "latest"
  },
  "engines": {
    "node": ">= 6"
  },
  "files": [
    "index.js",
    "scripts"
  ],
  "scripts": {
    "clean": "rm -rf node_modules",
    "coveralls": "nyc report --reporter=text-lcov | coveralls",
    "lint": "standard-markdown && standard",
    "postinstall": "node scripts/postinstall",
    "pretest": "npm run lint",
    "pretty": "prettier-standard index.js {core,test,bin}/**/*.js --single-quote",
    "test": "nyc mocha",
    "contributors": "(git-authors-cli && finepack && git add package.json && git commit -m 'build: contributors' --no-verify) || true",
    "postrelease": "npm run release:tags && npm run release:github && ci-publish",
    "prerelease": "npm run update:check && npm run contributors",
    "release:github": "conventional-github-releaser -p angular",
    "release:tags": "git push --follow-tags origin HEAD:master",
    "release": "standard-version -a",
    "update:check": "ncu -- --error-level 2",
    "update": "ncu -u"
  },
  "license": "MIT",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "*.js": [
      "prettier-standard"
    ],
    "package.json": [
      "finepack"
    ]
  },
  "standard": {
    "env": [
      "mocha"
    ]
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  }
}
